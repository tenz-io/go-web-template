            }
          }
        },
        "parameters": [
          {
            "name": "file",
            "description": "@inject_tag: bind:\"file,name=file\" validate:\"required,min_len=1\"",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "byte"
          },
          {
            "name": "key",
            "description": "@inject_tag: bind:\"form,name=key\"",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "ApiServer"
        ]
      }
    }
