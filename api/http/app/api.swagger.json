            }
          }
        },
        "parameters": [
          {
            "name": "key",
            "description": "@inject_tag: bind:\"uri,name=key\" validate=\"required,min_len=1,max_len=64\"",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "ApiServer"
        ]
      }
    },
