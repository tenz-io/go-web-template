              "$ref": "#/definitions/appHelloResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "name",
            "description": "@inject_tag: bind:\"query,name=name\" validate:\"required\" default:\"goer\"",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "ApiServer"
        ]
      }
    },
