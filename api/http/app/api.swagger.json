            }
          }
        },
        "parameters": [
          {
            "name": "username",
            "description": "@inject_tag: bind:\"form,name=username\" validate:\"required,non_blank,min_len=2,pattern=#abc123\"",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "password",
            "description": "@inject_tag: bind:\"form,name=password\" validate:\"required,non_blank,min_len=4\"",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "ApiServer"
        ]
      }
    },
    "/upload/image": {
